<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >

    <Button
        android:id="@+id/button_simple"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Simple"
        />

    <Button
        android:id="@+id/button_merge"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Merge demo"
        />

    <Button
        android:id="@+id/button_map"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Map demo"
        />

    <Button
        android:id="@+id/button_flat_map"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Flat Map demo"
        />

    <Button
        android:id="@+id/button_concat_with"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Concat With"
        />

</LinearLayout>
